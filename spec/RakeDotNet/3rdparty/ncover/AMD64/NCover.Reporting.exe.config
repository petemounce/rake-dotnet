<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <appSettings>
        <!--Set this key to 1 if you wish to opt out of our customer feedback portal-->
        <add key="customerfeedbackoptout" value="0"/>
        <!--
        Set this key to 1 to allow your token to be passed to support for debugging purposes
        Even if it is set to 1, we are only using this to help with a support case, the information is ignored for any other reason.
        -->
        <add key="customerfeedbackidentifyfordebug" value="0"/>
    </appSettings>
   
</configuration>